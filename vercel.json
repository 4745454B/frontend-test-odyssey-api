{
    "functions": {
        "api/**/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        { "src": "/(.*)",  "dest": "/api/index.php" },
        { "src": "/api/categories.json",  "dest": "/api/categories.json" },
        { "src": "/api/products.json",  "dest": "/api/products.json" }

  ]
}